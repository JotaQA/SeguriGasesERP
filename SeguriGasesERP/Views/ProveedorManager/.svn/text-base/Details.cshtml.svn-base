@model SeguriGasesERP.Models.Proveedor

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<fieldset>
    <legend>Proveedor</legend>

    <div class="display-label">RFC</div>
    <div class="display-field">@Model.RFC</div>

    <div class="display-label">RazonSocial</div>
    <div class="display-field">@Model.RazonSocial</div>

    <div class="display-label">Contacto</div>
    <div class="display-field">@Model.Contacto</div>

    <div class="display-label">Telefono</div>
    <div class="display-field">@Model.Telefono</div>

    <div class="display-label">Nextel</div>
    <div class="display-field">@Model.Nextel</div>

    <div class="display-label">Direccion</div>
    <div class="display-field">@Model.Direccion</div>

    <div class="display-label">Email</div>
    <div class="display-field">@Model.Email</div>
</fieldset>
<p>
    @Html.ActionLink("Edit", "Edit", new { id=Model.ID }) |
    @Html.ActionLink("Regresar a la Lista", "Index")
</p>

