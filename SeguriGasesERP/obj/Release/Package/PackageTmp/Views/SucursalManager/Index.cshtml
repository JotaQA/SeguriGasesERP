</p>
<table>
    <tr>
        <th></th>
        <th>
            Nombre
        </th>
        <th>
            DomicilioFiscal
        </th>
        <th>
            Telefono
        </th>
        <th>
            RFC
        </th>
        <th>
            Cuenta
        </th>
        <th>
            Password
        </th>
    </tr>

@foreach (var item in Model) {
